  0% { transform: rotate(-3deg); }
  2% { transform: rotate(20deg); }
  4% { transform: rotate(-15deg); }
  6% { transform: rotate(5deg); }
  8% { transform: rotate(-1deg); }
  100% { transform: rotate(0deg); }
}

