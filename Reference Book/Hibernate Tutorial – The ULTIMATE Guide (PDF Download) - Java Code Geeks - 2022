  0% { opacity: 0; }
  100% { opacity: 1; }
}

.smiotooltipster-update-fade { animation: 0.4s ease 0s 1 normal none running smiotooltipster-fading; }

@keyframes smiotooltipster-rotating { 
  25% { transform: rotate(-2deg); }
  75% { transform: rotate(2deg); }
  100% { transform: rotate(0deg); }
}

