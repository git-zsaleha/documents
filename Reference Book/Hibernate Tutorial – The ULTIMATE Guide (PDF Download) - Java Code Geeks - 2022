.smiotooltipster-ruler { inset: 0px; overflow: hidden; position: fixed; visibility: hidden; }

.smiotooltipster-fade { opacity: 0; transition-property: opacity; }

.smiotooltipster-fade.smiotooltipster-show { opacity: 1; }

.smiotooltipster-grow { transform: scale(0, 0); transition-property: transform; backface-visibility: hidden; }

.smiotooltipster-grow.smiotooltipster-show { transform: scale(1, 1); transition-timing-function: cubic-bezier(0.175, 0.885, 0.32, 1.15); }

.smiotooltipster-swing { opacity: 0; transform: rotateZ(4deg); transition-property: transform; }

.smiotooltipster-swing.smiotooltipster-show { opacity: 1; transform: rotateZ(0deg); transition-timing-function: cubic-bezier(0.23, 0.635, 0.495, 2.4); }

.smiotooltipster-fall { transition-property: top; transition-timing-function: cubic-bezier(0.175, 0.885, 0.32, 1.15); }

