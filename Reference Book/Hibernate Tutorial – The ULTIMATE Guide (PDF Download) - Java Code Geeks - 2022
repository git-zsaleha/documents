.fa-flip-both, .fa-flip-horizontal.fa-flip-vertical { transform: scale(-1); }

:root .fa-flip-both, :root .fa-flip-horizontal, :root .fa-flip-vertical, :root .fa-rotate-90, :root .fa-rotate-180, :root .fa-rotate-270 { filter: none; }

